{
  "plugins": {
    "web_search": {
      "enabled": true,
      "version": "1.0",
      "settings": {
        "api_key": "YOUR_SEARCH_API_KEY",
        "search_engine": "google"
      }
    },
    "image_analyzer": {
      "enabled": true,
      "version": "1.0",
      "settings": {
        "api_key": "YOUR_GEMINI_API_KEY",
        "model": "gemini-pro-vision"
      }
    },
    "youtube_caption": {
      "enabled": true,
      "version": "1.0",
      "settings": {
        "language": "zh-TW"
      }
    }
  }
} 